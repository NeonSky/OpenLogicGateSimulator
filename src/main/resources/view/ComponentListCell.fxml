<?import javafx.scene.Group?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" minHeight="80.0" minWidth="80.0"
         prefHeight="100.0"
         prefWidth="100.0" snapToPixel="false" styleClass="list-cell-background"
         stylesheets="@../css/ComponentListCell.css" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.121">
